    "moment-locales-webpack-plugin": "^1.2.0",
    "moment-timezone-data-webpack-plugin": "^1.5.1",
    "prettier": "^3.0.3",
    "raw-loader": "^4.0.2",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.14.0",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.3",
    "svg-url-loader": "^8.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
