    "clean-css": "^5.3.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cypress": "^13.1.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.6.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.48.0",
