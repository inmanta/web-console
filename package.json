    "react-syntax-highlighter": "^15.5.0",
    "styled-components": "^5.3.11",
    "xml-formatter": "^3.5.0"
  },
  "resolutions": {
    "json5": "^2.2.2",
    "terser-webpack-plugin": "^1.4.5",
    "@types/react": "^18.0.21",
    "prismjs": "^1.25.0",
    "trim": "^0.0.3",
    "glob-parent": "^5.1.2",
    "ansi-regex": "^5.0.1",
    "nth-check": "^2.0.1",
