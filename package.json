{
  "name": "@inmanta/web-console",
  "version": "1.4.0",
  "description": "Web Console for Inmanta Orchestrator",
  "main": "dist/index.js",
  "repository": "https://github.com/inmanta/web-console.git",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "test": "jest",
    "lint": "tslint -c ./tslint.json --project .",
    "format": "prettier --check --write ./src/**/*.{tsx,ts,js}",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "delete:reports": "rm -r cypress/reports/* || true",
    "precypress-test": "yarn run delete:reports",
    "cypress-test": "yarn start-test 'yarn start' http-get://localhost:9000 'cypress run'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@testing-library/react": "^9.1.4",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^5.1.0",
    "@types/victory": "^33.0.0",
    "@types/webpack": "^4.39.2",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "cypress": "^4.3.0",
    "cypress-multi-reporters": "^1.2.3",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.4.0",
    "fetch-mock": "^7.3.9",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^7.0.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-junit": "^8.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.23.1",
    "mochawesome": "^4.1.0",
    "mochawesome-report-generator": "^4.0.1",
    "node-fetch": "^2.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "prop-types": "^15.6.1",
    "raw-loader": "^3.1.0",
    "react": "^16.13.1",
    "react-axe": "3.3.0",
    "react-dom": "^16.13.1",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "start-server-and-test": "^1.10.6",
    "style-loader": "^1.0.0",
    "svg-url-loader": "^3.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@patternfly/react-core": "^3.112.3",
    "@patternfly/react-icons": "^3.14.7",
    "@patternfly/react-styles": "^3.5.22",
    "@patternfly/react-table": "^2.22.19",
    "dotenv-webpack": "^1.7.0",
    "easy-peasy": "^3.1.0",
    "keycloak-js": "6.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react-keycloak": "^6.1.1",
    "react-router-dom": "^5.1.2",
    "react-router-last-location": "^2.0.1"
  }
}
