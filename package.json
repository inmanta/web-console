{
  "name": "@inmanta/web-console",
  "version": "1.5.0",
  "description": "Web Console for Inmanta Orchestrator",
  "main": "dist/index.js",
  "repository": "https://github.com/inmanta/web-console.git",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "test": "jest",
    "format": "prettier --check --write './**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check './**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --max-warnings=0 './**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --max-warnings=0 './**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "delete:reports": "rm -r cypress/reports/* || true",
    "precypress-test": "yarn run delete:reports",
    "cypress-test": "yarn start-test 'yarn start' http-get://localhost:9000 'yarn cypress run'",
    "package-cleanup": "node clean_up_packages",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --check --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.7.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^5.1.7",
    "@types/victory": "^33.0.0",
    "@types/webpack": "^4.39.2",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^3.2.0",
    "cypress": "^4.3.0",
    "cypress-multi-reporters": "^1.2.3",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "fetch-mock": "^7.3.9",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.3.7",
    "imagemin": "^7.0.0",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^2.1.2",
    "jest-junit": "^8.0.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.23.1",
    "mochawesome": "^4.1.0",
    "mochawesome-report-generator": "^4.0.1",
    "node-fetch": "^2.6.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^2.2.1",
    "prop-types": "^15.6.1",
    "raw-loader": "^3.1.0",
    "react": "^16.13.1",
    "react-axe": "3.3.0",
    "react-dom": "^16.13.1",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "start-server-and-test": "^1.10.6",
    "style-loader": "^1.0.0",
    "svg-url-loader": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^6.2.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.84.4",
    "@patternfly/react-icons": "^4.7.22",
    "@patternfly/react-styles": "^4.7.22",
    "@patternfly/react-table": "^4.19.45",
    "@patternfly/react-tokens": "^4.9.22",
    "copy-to-clipboard": "^3.3.1",
    "dotenv-webpack": "^1.7.0",
    "easy-peasy": "^4.0.1",
    "keycloak-js": "6.0.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "react-keycloak": "^6.1.1",
    "react-router-dom": "^5.1.2",
    "react-router-last-location": "^2.0.1",
    "styled-components": "^5.2.1"
  },
  "resolutions": {
    "terser-webpack-plugin": "^1.4.5",
    "selfsigned": "^1.10.8"
  }
}
