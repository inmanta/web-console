{
  "name": "@inmanta/web-console",
  "version": "3.0.1",
  "description": "Web Console for Inmanta Orchestrator",
  "exports": "./dist/index.js",
  "repository": "https://github.com/inmanta/web-console.git",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "vite build",
    "start": "vite",
    "start:https": "HTTPS=true vite",
    "proxy": "lcp --proxyUrl http://localhost:8888",
    "proxy:https": "lcp --proxyUrl https://localhost:8888",
    "test": "vitest --run",
    "test:ci": "CI=true vitest --silent",
    "test:ui": "vitest --ui",
    "format": "prettier --check --write \"./**/*.{cjs,js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"./**/*.{cjs,js,jsx,ts,tsx}\"",
    "lint": "eslint --max-warnings=0 -c eslint.config.js",
    "lint:fix": "eslint --max-warnings=0  --quiet --fix",
    "build:bundle-profile": "vite build --mode analyze",
    "bundle-profile:analyze": "yarn build:bundle-profile",
    "clean": "rimraf dist",
    "delete:reports": "rm -r cypress/reports/* || true",
    "precypress-test": "yarn run delete:reports",
    "cypress-test:oss": "yarn cypress run --env edition=oss",
    "cypress-test:iso": "yarn cypress run",
    "cypress-test:keycloak": "yarn cypress run --env edition=oss,keycloak=true --config baseUrl='https://127.0.0.1:8888' --spec 'cypress/e2e/scenario-7.1-keycloak.cy.js'",
    "cypress-test:local-auth": "yarn cypress run --env edition=oss,local-auth=true --config baseUrl='https://127.0.0.1:8888' --spec 'cypress/e2e/scenario-7.2-local-auth.cy.js,cypress/e2e/scenario-7.3-user-management.cy.js'",
    "package-cleanup": "node clean_up_packages",
    "check-circular-deps": "madge --circular ./src/index.tsx",
    "install:orchestrator:keycloak": "sh ./shell-scripts/setup-auth.sh version='oss' release='dev' branch='oss_case_1'",
    "install:orchestrator:local-auth": "sh ./shell-scripts/setup-auth.sh version='oss' release='dev' branch='oss_case_1' auth='localauth'",
    "install:orchestrator": "sh ./shell-scripts/setup-orchestrator.sh flag='-ti'",
    "install:orchestrator:iso": "sh ./shell-scripts/setup-orchestrator.sh flag='-ti'",
    "install:orchestrator:oss": "sh ./shell-scripts/setup-orchestrator.sh flag='-ti' version='oss' release='dev' branch='oss_case_1'",
    "install:orchestrator:ci": "sh ./shell-scripts/setup-orchestrator.sh",
    "kill-server": "sh ./shell-scripts/clear-server.sh",
    "update:dist": "docker exec -u root inmanta_orchestrator rm -rf /usr/share/inmanta/web-console && docker cp dist inmanta_orchestrator:/usr/share/inmanta/web-console"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --check --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/"
  ],
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/jest-axe": "^3.5.9",
    "@types/json-bigint": "^1.0.4",
    "@types/loadable__component": "^5.13.10",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it-emoji": "^3.0.1",
    "@types/node": "^24.6.1",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cypress": "^13.6.3",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^2.0.5",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.8.1",
    "globals": "^16.3.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "madge": "^6.1.0",
    "markdown-it-emoji": "^3.0.0",
    "mocha": "^11.7.4",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^3.6.2",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.9",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-monaco-editor-esm": "^2.0.2",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0"
  },
  "browser": {
    "crypto": false
  },
  "dependencies": {
    "@inmanta/rappid": "^4.0.0",
