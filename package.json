{
  "name": "@inmanta/web-console",
  "version": "1.7.1",
  "description": "Web Console for Inmanta Orchestrator",
  "exports": "./dist/index.js",
  "repository": "https://github.com/inmanta/web-console.git",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "yarn clean",
    "build": "webpack --config webpack.prod.cjs",
    "start": "webpack serve --hot --color --progress --config webpack.dev.cjs",
    "test": "jest",
    "format": "prettier --check --write './**/*.{cjs,js,jsx,ts,tsx}'",
    "format:check": "prettier --check './**/*.{cjs,js,jsx,ts,tsx}'",
    "lint": "eslint --max-warnings=0 './**/*.{cjs,js,jsx,ts,tsx}'",
    "lint:fix": "eslint --max-warnings=0 './**/*.{cjs,js,jsx,ts,tsx}' --quiet --fix",
    "build:bundle-profile": "webpack --config webpack.prod.cjs --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "delete:reports": "rm -r cypress/reports/* || true",
    "precypress-test": "yarn run delete:reports",
    "cypress-test": "yarn start-test 'yarn start' http-get://localhost:9000 'yarn cypress run'",
    "package-cleanup": "node clean_up_packages",
    "storybook": "start-storybook --ci --port 6006",
    "build-storybook": "build-storybook",
    "check-circular-deps": "madge --circular ./src/index.tsx"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --check --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/"
  ],
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-typescript": "^7.16.1",
    "@storybook/addon-actions": "^v6.3.11",
    "@storybook/addon-essentials": "^v6.4.1",
    "@storybook/addon-links": "^v6.4.3",
    "@storybook/react": "^v6.4.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/fetch-mock": "^7.3.5",
    "@types/file-saver": "^2.0.4",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.11",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/styled-components": "^5.1.16",
    "@types/webpack": "^5.28.0",
    "@types/xml-formatter": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-loader": "^8.2.3",
    "clean-css": "^5.2.2",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "cypress": "^9.1.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "fetch-mock": "^9.11.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "imagemin": "^8.0.1",
    "jest": "^27.4.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.2",
    "madge": "^5.0.1",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.0.1",
    "mochawesome-report-generator": "^6.0.1",
    "node-fetch": "^3.1.0",
    "prettier": "^2.5.0",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-axe": "3.5.4",
    "react-dom": "^17.0.2",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.0",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-prune": "^0.10.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@patternfly/react-charts": "^6.28.4",
    "@patternfly/react-core": "^4.162.3",
    "@patternfly/react-icons": "^4.26.4",
    "@patternfly/react-styles": "^4.25.4",
    "@patternfly/react-table": "^4.44.4",
    "@patternfly/react-tokens": "^4.27.4",
    "copy-to-clipboard": "^3.3.1",
    "dotenv-webpack": "^7.0.3",
    "easy-peasy": "^5.0.4",
    "file-saver": "^2.0.5",
    "history": "5.1.0",
    "jszip": "^3.7.1",
    "keycloak-js": "15.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "process": "^0.11.10",
    "qs": "^6.10.1",
    "react-keycloak": "^6.1.1",
    "react-router-dom": "^6.0.2",
    "react-syntax-highlighter": "^15.4.5",
    "styled-components": "^5.3.3",
    "xml-formatter": "^2.5.1"
  },
  "resolutions": {
    "terser-webpack-plugin": "^1.4.5",
    "selfsigned": "^1.10.8"
  },
  "madge": {
    "fileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "webpackConfig": "./webpack.prod.js",
    "tsConfig": "./tsconfig.json",
    "layout": "dot",
    "excludeRegExp": [
      "Data/Store/ServiceInstancesSlice.ts"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions"
  ]
}
