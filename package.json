    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "imagemin": "^8.0.1",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.4",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "lint-staged": "^14.0.1",
    "madge": "^6.1.0",
    "mini-css-extract-plugin": "^2.7.6",
