{
  "name": "@inmanta/web-console",
  "version": "1.8.5",
  "description": "Web Console for Inmanta Orchestrator",
  "exports": "./dist/index.js",
  "repository": "https://github.com/inmanta/web-console.git",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "yarn clean",
    "build": "webpack --config webpack.prod.cjs",
    "start": "webpack serve --hot --color --progress --config webpack.dev.cjs",
    "test": "jest",
    "format": "prettier --check --write './**/*.{cjs,js,jsx,ts,tsx}'",
    "format:check": "prettier --check './**/*.{cjs,js,jsx,ts,tsx}'",
    "lint": "eslint --max-warnings=0 './**/*.{cjs,js,jsx,ts,tsx}'",
    "lint:fix": "eslint --max-warnings=0 './**/*.{cjs,js,jsx,ts,tsx}' --quiet --fix",
    "build:bundle-profile": "webpack --config webpack.prod.cjs --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "delete:reports": "rm -r cypress/reports/* || true",
    "precypress-test": "yarn run delete:reports",
    "cypress-test": "yarn start-test 'yarn start' http-get://localhost:9000 'yarn cypress run'",
    "package-cleanup": "node clean_up_packages",
    "storybook": "start-storybook --ci --port 6006",
    "build-storybook": "build-storybook",
    "check-circular-deps": "madge --circular ./src/index.tsx"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --check --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/"
  ],
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@storybook/addon-actions": "^v6.4.15",
    "@storybook/addon-essentials": "^v6.4.15",
    "@storybook/addon-links": "^v6.4.15",
    "@storybook/react": "^v6.4.15",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/fetch-mock": "^7.3.5",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^27.4.0",
    "@types/json-bigint": "^1.0.1",
    "@types/lodash": "^4.14.178",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.13",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/styled-components": "^5.1.21",
    "@types/webpack": "^5.28.0",
    "@types/xml-formatter": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-loader": "^8.2.3",
    "clean-css": "^5.2.4",
    "copy-webpack-plugin": "^10.2.2",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cypress": "^9.3.1",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.4",
    "fetch-mock": "^9.11.0",
    "file-loader": "^6.2.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "imagemin": "^8.0.1",
    "jest": "^27.4.7",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.3.2",
    "madge": "^5.0.1",
    "mini-css-extract-plugin": "^2.5.3",
    "mocha": "^9.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.0.1",
    "mochawesome-report-generator": "^6.0.1",
    "moment-locales-webpack-plugin": "^1.2.0",
    "moment-timezone-data-webpack-plugin": "^1.5.0",
    "node-fetch": "^3.2.0",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-axe": "3.5.4",
    "react-dom": "^17.0.2",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.0",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-prune": "^0.10.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.67.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@patternfly/react-charts": "^6.45.15",
    "@patternfly/react-core": "^4.192.15",
    "@patternfly/react-icons": "^4.26.4",
    "@patternfly/react-styles": "^4.25.4",
    "@patternfly/react-table": "^4.61.15",
    "@patternfly/react-tokens": "^4.27.4",
    "copy-to-clipboard": "^3.3.1",
    "dotenv-webpack": "^7.1.0",
    "easy-peasy": "^5.0.4",
    "file-saver": "^2.0.5",
    "history": "5.2.0",
    "json-bigint": "^1.0.0",
    "jszip": "^3.7.1",
    "keycloak-js": "16.1.1",
    "lodash": "^4.17.21",
