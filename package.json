{
  "name": "@inmanta/web-console",
  "version": "1.10.1",
  "description": "Web Console for Inmanta Orchestrator",
  "exports": "./dist/index.js",
  "repository": "https://github.com/inmanta/web-console.git",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "yarn clean",
    "build": "webpack --config webpack.prod.cjs",
    "start": "webpack serve --hot --color --progress --config webpack.dev.cjs",
    "test": "jest --coverage=true",
    "format": "prettier --check --write './**/*.{cjs,js,jsx,ts,tsx}'",
    "format:check": "prettier --check './**/*.{cjs,js,jsx,ts,tsx}'",
    "lint": "eslint --max-warnings=0 './**/*.{cjs,js,jsx,ts,tsx}'",
    "lint:fix": "eslint --max-warnings=0 './**/*.{cjs,js,jsx,ts,tsx}' --quiet --fix",
    "build:bundle-profile": "webpack --config webpack.prod.cjs --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "delete:reports": "rm -r cypress/reports/* || true",
    "precypress-test": "yarn run delete:reports",
    "cypress-test": "yarn start-test 'yarn start' http-get://localhost:9000 'yarn cypress run'",
    "package-cleanup": "node clean_up_packages",
    "storybook": "start-storybook --ci --port 6006",
    "build-storybook": "build-storybook",
    "check-circular-deps": "madge --circular ./src/index.tsx"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --check --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/"
  ],
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/plugin-transform-typescript": "^7.17.12",
    "@storybook/addon-actions": "^v6.4.22",
    "@storybook/addon-essentials": "^v6.5.0",
    "@storybook/addon-links": "^v6.5.0",
    "@storybook/react": "^v6.4.22",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@types/fetch-mock": "^7.3.5",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^27.5.0",
    "@types/json-bigint": "^1.0.1",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.34",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.1",
    "@types/styled-components": "^5.1.25",
    "@types/webpack": "^5.28.0",
    "@types/xml-formatter": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-loader": "^8.2.5",
    "clean-css": "^5.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cypress": "^9.6.1",
    "cypress-multi-reporters": "^1.6.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-testing-library": "^5.5.0",
    "fetch-mock": "^9.11.0",
    "file-loader": "^6.2.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "imagemin": "^8.0.1",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^13.2.0",
    "lint-staged": "^12.4.1",
    "madge": "^5.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "moment-timezone-data-webpack-plugin": "^1.5.0",
    "node-fetch": "^3.2.4",
    "prettier": "^2.6.2",
    "raw-loader": "^4.0.2",
    "react-axe": "3.5.4",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.0",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.3.0",
    "ts-prune": "^0.10.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@patternfly/react-charts": "^6.67.1",
    "@patternfly/react-core": "^4.206.2",
    "@patternfly/react-icons": "^4.53.16",
    "@patternfly/react-styles": "^4.56.2",
    "@patternfly/react-table": "^4.83.1",
    "@patternfly/react-tokens": "^4.66.1",
    "copy-to-clipboard": "^3.3.1",
    "easy-peasy": "^5.0.4",
    "file-saver": "^2.0.5",
    "history": "5.3.0",
    "json-bigint": "^1.0.0",
    "jszip": "^3.9.1",
    "keycloak-js": "18.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "process": "^0.11.10",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^17.0.2",
    "react-keycloak": "^6.1.1",
    "react-router-dom": "^6.3.0",
    "react-syntax-highlighter": "^15.5.0",
    "styled-components": "^5.3.5",
    "xml-formatter": "^2.6.1"
  },
  "resolutions": {
    "terser-webpack-plugin": "^1.4.5",
    "selfsigned": "^1.10.8",
    "immer": "^9.0.6",
    "prismjs": "^1.25.0",
    "trim": "^0.0.3",
    "glob-parent": "^5.1.2",
    "ansi-regex": "^5.0.1",
    "nth-check": "^2.0.1",
    "browserslist": "^4.16.5",
    "json-schema": "^0.4.0",
    "node-forge": "^1.0.0",
    "cross-fetch": "^3.1.5",
    "node-fetch": "^2.6.7",
    "minimist": "^1.2.6"
  },
  "madge": {
    "fileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "webpackConfig": "./webpack.prod.js",
    "tsConfig": "./tsconfig.json",
    "layout": "dot",
    "excludeRegExp": [
      "Data/Store/ServiceInstancesSlice.ts"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions"
  ]
}
