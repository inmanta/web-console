{
  "name": "@inmanta/web-console",
  "version": "1.6.1",
  "description": "Web Console for Inmanta Orchestrator",
  "main": "dist/index.js",
  "repository": "https://github.com/inmanta/web-console.git",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "yarn clean",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack serve --hot --color --progress --config webpack.dev.js",
    "test": "jest",
    "format": "prettier --check --write './**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check './**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --max-warnings=0 './**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --max-warnings=0 './**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "delete:reports": "rm -r cypress/reports/* || true",
    "precypress-test": "yarn run delete:reports",
    "cypress-test": "yarn start-test 'yarn start' http-get://localhost:9000 'yarn cypress run'",
    "package-cleanup": "node clean_up_packages",
    "storybook": "start-storybook --ci --port 6006",
    "build-storybook": "build-storybook",
    "check-circular-deps": "madge --circular ./src/index.tsx"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --check --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@storybook/addon-actions": "^v6.3.0",
    "@storybook/addon-essentials": "^v6.3.1",
    "@storybook/addon-links": "^v6.3.1",
    "@storybook/react": "^v6.3.1",
    "@testing-library/dom": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/fetch-mock": "^7.3.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-syntax-highlighter": "^13.5.1",
    "@types/styled-components": "^5.1.10",
    "@types/victory": "^33.0.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "cypress": "^7.6.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "fetch-mock": "^9.11.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "imagemin": "^8.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.2.0",
    "lint-staged": "^11.0.0",
    "madge": "^5.0.1",
    "mini-css-extract-plugin": "^1.6.2",
    "mocha": "^9.0.1",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.2.2",
    "mochawesome-report-generator": "^5.2.0",
    "node-fetch": "^2.6.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.3.2",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-axe": "3.5.4",
    "react-dom": "^17.0.2",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.0",
    "start-server-and-test": "^1.12.5",
    "style-loader": "^3.0.0",
    "svg-url-loader": "^7.1.1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.3",
    "ts-prune": "^0.9.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.40.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@patternfly/react-charts": "^6.15.3",
    "@patternfly/react-core": "^4.135.0",
    "@patternfly/react-icons": "^4.10.11",
    "@patternfly/react-styles": "^4.10.2",
    "@patternfly/react-table": "^4.29.0",
    "@patternfly/react-tokens": "^4.11.12",
    "copy-to-clipboard": "^3.3.1",
    "dotenv-webpack": "^7.0.3",
    "easy-peasy": "^5.0.3",
    "history": "5.0.0",
    "keycloak-js": "14.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "process": "^0.11.10",
    "qs": "^6.10.1",
    "react-keycloak": "^6.1.1",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.3",
    "styled-components": "^5.3.0"
  },
  "resolutions": {
    "terser-webpack-plugin": "^1.4.5",
    "selfsigned": "^1.10.8"
  },
  "madge": {
    "fileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "webpackConfig": "./webpack.prod.js",
    "tsConfig": "./tsconfig.json",
    "layout": "dot",
    "excludeRegExp": [
      "Data/Store/ServiceInstancesSlice.ts"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions"
  ]
}