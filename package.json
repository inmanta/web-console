{
  "name": "@inmanta/web-console",
  "version": "1.12.2",
  "description": "Web Console for Inmanta Orchestrator",
  "exports": "./dist/index.js",
  "repository": "https://github.com/inmanta/web-console.git",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "yarn clean",
    "build": "webpack --config webpack.prod.cjs",
    "start": "webpack serve --hot --color --progress --config webpack.dev.cjs",
    "test": "jest --coverage=true --maxWorkers=50%",
    "test:ci": "jest --coverage=true --runInBand",
    "format": "prettier --check --write \"./**/*.{cjs,js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"./**/*.{cjs,js,jsx,ts,tsx}\"",
    "lint": "eslint --max-warnings=0 \"./**/*.{cjs,js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --max-warnings=0 \"./**/*.{cjs,js,jsx,ts,tsx}\" --quiet --fix",
    "build:bundle-profile": "webpack --config webpack.prod.cjs --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "delete:reports": "rm -r cypress/reports/* || true",
    "precypress-test": "yarn run delete:reports",
    "cypress-test": "yarn cypress run",
    "package-cleanup": "node clean_up_packages",
    "check-circular-deps": "madge --circular ./src/index.tsx",
    "setup-server:lsm": "sh ./shell-scripts/setup-server.sh windows lts",
    "setup-server:lsm:linux": "sh ./shell-scripts/setup-server.sh linux lts",
    "setup-server:lsm:ci": "sh ./shell-scripts/setup-server.sh ci lts",
    "kill-server:lsm": "sh ./shell-scripts/clear-server.sh"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --check --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/"
  ],
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-typescript": "^7.20.7",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/fetch-mock": "^7.3.5",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.4.0",
    "@types/json-bigint": "^1.0.1",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/styled-components": "^5.1.26",
    "@types/webpack": "^5.28.0",
    "@types/xml-formatter": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-loader": "^9.1.2",
    "clean-css": "^5.3.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "cypress": "^12.4.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.6.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.1",
    "fetch-mock": "^9.11.0",
    "file-loader": "^6.2.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "imagemin": "^8.0.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.1.0",
    "madge": "^5.0.1",
    "mini-css-extract-plugin": "^2.7.0",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "moment-timezone-data-webpack-plugin": "^1.5.1",
    "prettier": "^2.8.3",
    "raw-loader": "^4.0.2",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^4.1.2",
    "start-server-and-test": "^1.15.2",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^8.0.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-prune": "^0.10.3",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-version-file": "^0.1.7"
  },
  "dependencies": {
    "@patternfly/react-charts": "^6.94.15",
    "@patternfly/react-core": "^4.267.6",
    "@patternfly/react-icons": "^4.93.3",
    "@patternfly/react-styles": "^4.92.3",
    "@patternfly/react-table": "^4.112.6",
    "@patternfly/react-tokens": "^4.93.10",
    "@react-keycloak/web": "^3.4.0",
    "copy-to-clipboard": "^3.3.3",
    "easy-peasy": "^5.2.0",
    "file-saver": "^2.0.5",
    "history": "^5.3.0",
    "json-bigint": "^1.0.0",
    "jszip": "^3.10.1",
    "keycloak-js": "^20.0.2",
    "lint": "^0.8.19",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "process": "^0.11.10",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2",
    "react-syntax-highlighter": "^15.5.0",
    "styled-components": "^5.3.6",
    "xml-formatter": "^3.2.0"
  },
  "resolutions": {
    "json5": "^2.2.2",
    "terser-webpack-plugin": "^1.4.5",
    "@types/react": "^18.0.21",
    "prismjs": "^1.25.0",
    "trim": "^0.0.3",
    "glob-parent": "^5.1.2",
    "ansi-regex": "^5.0.1",
    "nth-check": "^2.0.1",
    "browserslist": "^4.16.5",
    "json-schema": "^0.4.0",
    "node-forge": "^1.0.0",
    "minimist": "^1.2.6",
    "simple-git": "^3.5.0",
    "async": "^3.2.2",
    "terser": "^5.14.2",
    "loader-utils": "2.0.4 || 1.4.2"
  },
  "madge": {
    "fileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "webpackConfig": "./webpack.prod.js",
    "tsConfig": "./tsconfig.json",
    "layout": "dot",
    "excludeRegExp": [
      "Data/Store/ServiceInstancesSlice.ts"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions"
  ]
}
