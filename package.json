    "nanoid": "^3.3.8",
    "node-forge": "^1.0.0",
    "nth-check": "^2.0.1",
    "path-to-regexp@^6.2.0": "8.0.0",
    "prismjs": "^1.30.0",
    "serialize-javascript": "6.0.2",
    "simple-git": "^3.5.0",
