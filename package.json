    "react-syntax-highlighter": "^15.4.5",
    "styled-components": "^5.3.3",
    "xml-formatter": "^2.6.0"
  },
  "resolutions": {
    "terser-webpack-plugin": "^1.4.5",
    "selfsigned": "^1.10.8",
    "immer": "^9.0.6",
    "prismjs": "^1.25.0",
    "trim": "^0.0.3",
    "glob-parent": "^5.1.2",
    "ansi-regex": "^5.0.1",
    "nth-check": "^2.0.1",
    "browserslist": "^4.16.5",
    "json-schema": "^0.4.0",
    "node-forge": "^1.0.0",
    "cross-fetch": "^3.1.5",
    "node-fetch": "^2.6.7",
    "minimist": "^1.2.6"
  },
  "madge": {
    "fileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "webpackConfig": "./webpack.prod.js",
    "tsConfig": "./tsconfig.json",
    "layout": "dot",
    "excludeRegExp": [
      "Data/Store/ServiceInstancesSlice.ts"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions"
  ]
}
